{
  "name": "2nd-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "#start": "UI server. HMR is enabled in dev mode.",
    "start": "node dist/server.js",
    "#lint": "Runs ESLint on all relavent files",
    "lint": "eslint server src browser --ext js, jsx",
    "#compile": "Generate JS bundles for production. Use with start.",
    "compile": "webpack --mode production",
    "#watch-server-hmr": "Recompile server HMR bundle on changes.",
    "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
    "#dev-all": "Dev mode: watch for server changes and start UI server(Linux&MacOS)",
    "dev-all": "rm dist/* && npm run watch-server-hmr & sleep 5 && npm start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "serialize-javascript": "^3.1.0",
    "source-map-support": "^0.5.18",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
